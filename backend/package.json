{
  "name": "tnx-light-backend",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && c8 node --test -r ts-node/register \"test/**/*.ts\"",
    "start": "node ./dist/index.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:start\"",
    "codegen": "kysely-codegen --out-file ./src/types/db/db.d.ts --dialect postgres --env-file ./.env"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/multipart": "^9.0.1",
    "@fastify/rate-limit": "^10.1.1",
    "@fastify/sensible": "^6.0.0",
    "@fastify/swagger": "^9.2.0",
    "@fastify/swagger-ui": "^5.1.0",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.0.1",
    "fastify-plugin": "^5.0.0",
    "kysely": "^0.27.4",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@sinclair/typebox": "^0.33.17",
    "@types/node": "^22.1.0",
    "@types/pg": "^8.11.10",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "fastify-tsconfig": "^2.0.0",
    "kysely-codegen": "^0.17.0",
    "ts-node": "^10.4.0",
    "typescript": "^5.2.2"
  }
}
